<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>Overlay Framework</Title>
    <Authors>Macha Celeste</Authors>
    <Company>Celestial Corp</Company>
    <Version>0.1.8</Version>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DebugType>none</DebugType>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Chat.html" />
    <None Remove="Notifications.html" />
    <None Remove="Overlays\EmoteWall.html" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Overlays\Chat.html" />
    <EmbeddedResource Include="Overlays\EmoteWall.html" />
    <EmbeddedResource Include="Overlays\Notifications.html" />
  </ItemGroup>

</Project>
